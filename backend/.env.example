# Production Environment Variables
# Copy this to .env and fill in your values

# JWT Secret - MUST be 256-bit (32 characters minimum) for production
JWT_SECRET=your-super-secret-256-bit-key-here-change-this-in-production

# JWT Token Expiration (milliseconds)
JWT_EXPIRATION=86400000

# CORS Allowed Origins (comma-separated)
ALLOWED_ORIGINS=https://your-frontend-domain.com,https://www.your-frontend-domain.com

# Database (for production)
SPRING_DATASOURCE_URL=jdbc:postgresql://your-db-host/your-db-name
SPRING_DATASOURCE_USERNAME=your-db-user
SPRING_DATASOURCE_PASSWORD=your-db-password

# Spring Profile
SPRING_PROFILES_ACTIVE=prod